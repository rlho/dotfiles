let g:hybrid_use_Xresources = 1
let g:indent_guides_enable_on_vim_startup = 1
syntax on
set hlsearch
set ruler
set noswapfile
set title
set incsearch
set autoread
set ignorecase
set smartcase
set wrapscan
" オートインデント設定
set tabstop=2
set autoindent
set smartindent
set expandtab
set shiftwidth=2
" 個人設定"
set number

" tab
map  <C-t><C-n> :tabnew<CR>
map  <C-t><C-w> :tabclose<CR>
map  <C-t><C-t> :tabn<CR>
map  <C-t><C-p> :tabp<CR>
